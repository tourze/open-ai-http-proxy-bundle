{
  "name": "tourze/open-ai-http-proxy-bundle",
  "description": "Symfony bundle for proxying OpenAI API compatible services with fallback and model mapping",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "doctrine/doctrine-bundle": "^2.13",
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-client-contracts": "^3.6",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/messenger": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/service-contracts": "^3.6",
    "symfony/string": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/access-key-bundle": "0.1.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/http-forward-bundle": "0.0.*",
    "tourze/symfony-dependency-service-loader": "1.*",
    "tourze/symfony-routing-auto-loader-bundle": "1.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/phpunit-symfony-web-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\OpenAiHttpProxyBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\OpenAiHttpProxyBundle\\Tests\\": "tests"
    }
  }
}